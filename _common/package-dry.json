{
    "dry": {
        "extends": "../_common/package-dry.base.json"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/runtime-corejs3": "^7.15.3",
        "@google/clasp": "^2.4.1",
        "@rollup/plugin-babel": "^5.3.0",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-json": "^4.1.0",
        "@rollup/plugin-node-resolve": "^13.0.4",
        "@rollup/plugin-typescript": "^8.2.5",
        "regenerator-runtime": "^0.13.9",
        "rollup": "^2.56.3",
        "rollup-plugin-inject-process-env": "^1.3.1",
        "tslib": "^2.3.1",
        "typescript": "^4.3.5"
    },
    "license": "MIT",
    "scripts": {
        "prepare-utilities": "yarn --cwd ../helpers-utilities && yarn --cwd ../helpers-utilities run build",
        "prepare-logging": "yarn --cwd ../helpers-logging && yarn --cwd ../helpers-logging run build",
        "prepare-caching": "yarn --cwd ../helpers-caching && yarn --cwd ../helpers-caching run build",
        "prepare-exchange": "yarn --cwd ../helpers-exchange && yarn --cwd ../helpers-exchange run build",
        "prepare-exporting": "yarn --cwd ../helpers-exporting && yarn --cwd ../helpers-exporting run build",
        "prepare-triggering": "yarn --cwd ../helpers-triggering && yarn --cwd ../helpers-triggering run build",
        "prepare-iteration": "yarn --cwd ../helpers-iteration && yarn --cwd ../helpers-iteration run build",
        "prepare-filesystem": "yarn --cwd ../helpers-filesystem && yarn --cwd ../helpers-filesystem run build",
        "prepare-querying": "yarn --cwd ../helpers-querying && yarn --cwd ../helpers-querying run build",
        "prepare": "yarn run prepare-utilities && yarn run prepare-logging && yarn run prepare-caching && yarn run prepare-exchange && yarn run prepare-exporting && yarn run prepare-triggering && yarn run prepare-iteration && yarn run prepare-filesystem && yarn run prepare-querying",
        "build-only": "eslint . --ext .ts && rollup -c",
        "build": "yarn run prepare && yarn run build-only",
        "push-only": "clasp push",
        "push": "yarn run build && yarn run push-only"
    },
    "version": "1.0.0"
}
